{
    "name": "nevergetfancy",
    "version": "1.0.0",
    "description": "Verify how many points you lost (or gained!) by getting fancy in Fantasy Premier League",
    "main": "index.js",
    "repository": "https://github.com/jamjar919/nevergetfancy.git",
    "author": "James Paterson",
    "license": "None",
    "engines": {
        "node": "22.14.0"
    },
    "scripts": {
        "build:client": "next build ./src/client",
        "build:client:dev": "next dev ./src/client",
        "start:client": "next start ./src/client",
        "build:server": "npx webpack --config webpack.server.config.js --mode=production",
        "build:server:dev": "npx webpack -w --config webpack.server.config.js --mode=development",
        "start:server": "node ./dist/server.js",
        "graphql": "npx graphql-codegen --config ./src/graphql/graphql-codegen.yml",
        "build:indexer": "npx webpack --config 'webpack.terminal.config.js'",
        "run:indexer": "node './dist/indexer/indexer.js'",
        "format:write": "prettier --write .",
        "format:check": "prettier --check ."
    },
    "dependencies": {
        "@apollo/client": "^3.13.4",
        "@apollo/experimental-nextjs-app-support": "^0.11.11",
        "@apollo/server": "^4.11.3",
        "classnames": "^2.5.1",
        "cors": "^2.8.5",
        "dotenv": "^16.4.7",
        "express": "^4.21.2",
        "graphql": "^16.10.0",
        "next": "^15.2.2",
        "node-fetch": "^3.3.2",
        "pg": "^8.14.1",
        "promise-retry": "^2.0.1",
        "react": "^19.0.0",
        "react-dom": "^19.0.0",
        "react-modal": "^3.16.3",
        "ts-node": "^10.9.2",
        "yargs": "^17.7.2"
    },
    "devDependencies": {
        "@graphql-codegen/cli": "^5.0.5",
        "@graphql-codegen/schema-ast": "^4.1.0",
        "@graphql-codegen/typescript": "^4.1.5",
        "@graphql-codegen/typescript-operations": "^4.5.1",
        "@graphql-codegen/typescript-react-apollo": "^4.3.2",
        "@graphql-codegen/typescript-resolvers": "^4.4.4",
        "@trivago/prettier-plugin-sort-imports": "^5.2.2",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/node": "22.13.10",
        "@types/pg": "^8.11.11",
        "@types/promise-retry": "^1.1.6",
        "@types/react": "19.0.10",
        "@types/react-modal": "^3.16.3",
        "@types/yargs": "^17.0.33",
        "eslint": "^9",
        "nodemon-webpack-plugin": "^4.8.2",
        "prettier": "^3.5.3",
        "sass": "^1.85.1",
        "ts-loader": "^9.5.2",
        "typescript": "^5.8.2",
        "webpack": "^5.98.0",
        "webpack-cli": "^6.0.1"
    }
}
